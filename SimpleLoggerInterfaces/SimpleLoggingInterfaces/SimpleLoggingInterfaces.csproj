<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Copyright></Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Description>Interface used to determine logging message type. Objects created to be passed from SimpleLoggerClient to SimpleLoggerService</Description>
    <PackageProjectUrl></PackageProjectUrl>
    <RepositoryUrl>https://github.com/Zeroshi/SimpleLoggingInterfaces</RepositoryUrl>
    <RepositoryType>GitHub</RepositoryType>
    <PackageTags>Logging, Interfaces, Simple, RabbitMQ, Enterprise, Cloud</PackageTags>
    <Authors>Thomas Nelson</Authors>
    <Version>1.1.2</Version>
    <PackageReleaseNotes>Added LogLevel.Off so that the user can turn off logs being send to RabbitMQ</PackageReleaseNotes>
  </PropertyGroup>
</Project>